{
  "name": "@geoblink/apidoc-plugin-ts",
  "version": "1.0.0",
  "description": "A plugin for apidoc leveraging TypeScript interfaces.",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "apidoc": "apidoc -i test/ -o test/out",
    "copy:plugin": "rm -rf node_modules/apidoc-plugin-ts && copyfiles package.json './dist/**/*' node_modules/apidoc-plugin-ts/",
    "lint": "tslint --project tsconfig.json",
    "test": "mocha test/**/test.ts",
    "prepare": "rm -rf dist && tsc -p src/tsconfig.json",
    "prepublishOnly": "run-s prepare lint test",
    "pretest": "rm -rf test/out && run-s prepare copy:plugin apidoc"
  },
  "authors": [
    "Tom Grey",
    "Geoblink"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "bugs": {
    "url": "https://github.com/geoblink/apidoc-plugin-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/geoblink/apidoc-plugin-ts.git"
  },
  "keywords": [
    "apidoc",
    "apidoc plugin",
    "typescript"
  ],
  "peerDependencies": {
    "apidoc": "^0.17.7"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.0",
    "apidoc": "^0.17.7",
    "chai": "^4.2.0",
    "copyfiles": "^2.1.0",
    "mocha": "^6.0.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-config-standard": "^8.0.1"
  },
  "dependencies": {
    "ts-morph": "1.3.3",
    "typescript": "3.4.2"
  }
}
